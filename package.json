{
  "name": "carstournaments-backend",
  "version": "0.7.9",
  "scripts": {
    "start": "NODE_ENV=production nodemon",
    "start:dev": "NODE_ENV=development nodemon",
    "lint": "eslint \"**/*.{ts,tsx}\" --fix",
    "test": "NODE_ENV=test jest --verbose  --silent",
    "test:watch": "NODE_ENV=test jest --coverage --watchAll --detectOpenHandles",
    "prepare:husky": "husky install",
    "prettier:list": "prettier --list-different \"./{e2e,src}/**/*.{scss,ts}\" || ECHO .",
    "prettier:write": "prettier --write \"./{e2e,src}/**/*.{scss,ts}\"",
    "install:force": "sh ./scripts/install.sh",
    "update": "sh ./scripts/utils/update.sh",
    "update-ci": "sh ./scripts/update-ci.sh",
    "build:admin": "sh ./scripts/build-admin.sh",
    "build:app": "sh ./scripts/build-app.sh",
    "sign:apk": "sh ./scripts/utils/apk.sh",
    "sign:apk:install": "sh ./scripts/utils/apk.sh install",
    "sign:aab": "sh ./scripts/utils/aab.sh",
    "ota": "sh ./scripts/ota.sh",
    "deploy:app": "sh ./scripts/utils/pwa.sh app",
    "deploy:admin": "sh ./scripts/utils/pwa.sh admin",
    "db:proToUat": "sh ./scripts/utils/database.sh proToUat",
    "db:uatToPro": "sh ./scripts/utils/database.sh uatToPro",
    "db:uatToTest": "sh ./scripts/utils/database.sh uatToTest",
    "db:proToTest": "sh ./scripts/utils/database.sh proToTest",
    "img:proToLocal": "sh ./scripts/utils/images.sh proToLocal",
    "img:localToPro": "sh ./scripts/utils/images.sh localToPro",
    "ct": "sh ./scripts/ct.sh"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@google-analytics/data": "^2.8.0",
    "@ngneat/falso": "~4.0.0",
    "@types/node-cron": "^3.0.1",
    "axios": "^0.27.2",
    "bcrypt": "~5.0.1",
    "class-transformer": "~0.5.1",
    "class-validator": "~0.13.2",
    "compression": "~1.7.4",
    "cors": "~2.8.5",
    "date-fns": "^2.29.1",
    "dotenv": "~16.0.0",
    "express": "~4.17.3",
    "express-prom-bundle": "^6.4.1",
    "express-rate-limit": "^6.4.0",
    "express-status-monitor": "^1.3.4",
    "fake-user": "~1.1.0",
    "fcm-node": "^1.6.1",
    "ioredis": "^5.2.0",
    "jsonwebtoken": "~8.5.1",
    "method-override": "~3.0.0",
    "moment": "^2.29.4",
    "mongoose": "^6.5.2",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "node-schedule": "^2.1.0",
    "path": "~0.12.7",
    "prom-client": "^14.0.1",
    "python-shell": "~3.0.1",
    "request": "~2.88.2",
    "rxjs": "^7.5.5",
    "serve-index": "~1.9.1",
    "sharp": "~0.30.3",
    "typescript": "~4.6.3",
    "uuid": "^8.3.2",
    "winston": "^3.7.2"
  },
  "devDependencies": {
    "@types/bcrypt": "~5.0.0",
    "@types/compression": "~1.7.2",
    "@types/cors": "~2.8.12",
    "@types/express": "~4.17.13",
    "@types/jest": "^28.1.1",
    "@types/jsonwebtoken": "~8.5.8",
    "@types/method-override": "~0.0.32",
    "@types/morgan": "^1.9.3",
    "@types/multer": "~1.4.7",
    "@types/node-schedule": "^2.1.0",
    "@types/request": "~2.48.8",
    "@types/serve-index": "~1.9.1",
    "@types/sharp": "~0.30.1",
    "@types/supertest": "^2.0.12",
    "@types/uuid": "^8.3.4",
    "@types/validator": "~13.7.2",
    "@typescript-eslint/eslint-plugin": "^5.26.0",
    "@typescript-eslint/parser": "^5.27.0",
    "eslint": "^8.23.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-jest": "^27.0.1",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.1",
    "jest": "^28.1.3",
    "lint-staged": "^13.0.3",
    "mongodb-memory-server": "^8.9.0",
    "nodemon": "~2.0.15",
    "prettier": "~2.6.2",
    "pretty-quick": "~3.1.3",
    "supertest": "^6.2.4",
    "ts-jest": "^28.0.5",
    "ts-node": "^10.8.1",
    "tsconfig-paths": "^4.0.0"
  },
  "eslintIgnore": [
    "*.spec.ts"
  ],
  "keywords": [
    "expressjs",
    "typescript",
    "mongoose",
    "nodemon",
    "prettier",
    "multer",
    "morgan",
    "bcrypt"
  ],
  "lint-staged": {
    "*.ts": "eslint --fix"
  },
  "nodemonConfig": {
    "ignore": [
      "**/*.spec.ts",
      ".git",
      "node_modules"
    ],
    "watch": [
      "src"
    ],
    "exec": "node -r tsconfig-paths/register -r ts-node/register ./src/server.ts",
    "ext": "ts, js"
  }
}
